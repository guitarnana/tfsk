<Window x:Class="tfsk.UsageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="tfsk usage" Height="575" Width="640">
    <Grid>
		<StackPanel>
			<TextBlock x:Name="tbUsage" FontSize="13" Margin="15" />
			<Button Click="Button_Click" VerticalAlignment="Center" HorizontalAlignment="Center" Width="80" IsDefault="True" IsCancel="True" Padding="5">OK</Button>
		</StackPanel>
	</Grid>
</Window>
